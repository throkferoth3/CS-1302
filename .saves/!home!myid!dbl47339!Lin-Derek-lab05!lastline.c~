#include <stdio.h>
#include <stdlib.h>

int main(int argc, char *argv[])
{
    FILE *fp;
	char* filename = argv[1];
	fp = fopen(filename, "r");
    if (fp == NULL) perror("open");

	char buffer[100];

    while (!feof(fp)) {
        fgets(buffer, 100, fp);
    }

    printf(buffer);

	return EXIT_SUCCESS;
} // main
